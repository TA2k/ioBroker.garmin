  "encryptedNative": ["password"],
  "protectedNative": ["password"],
  "native": {
    "username": "",
    "password": "",
    "interval": 30,
    "mfa": "",
    "adhoc": false
  },
  "objects": [],
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Device or service connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "cookie",
      "type": "state",
      "common": {
        "role": "state",
        "name": "Cookie state",
        "type": "string",
        "read": true,
        "write": false
      },
      "native": {}
    }
  ]
